<Window x:Class="AnalizaMorfologicznaObrazow_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnalizaMorfologicznaObrazow_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800">
    <Grid>
        <Image Source="/Resources/Sample_ready.jpg" ></Image>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button x:Name="BtnFiltryXY" Content="FiltersXY" Width="100" Click="BtnClickFiltry" />
            <Button x:Name="BtnPodstawyMorfologi" Content="Morphology Basics" Width="120" Margin="10,0,0,0" Click="BtnClickPodstawyMorfologi" />
        </StackPanel>
       
        <Frame x:Name="Main" Margin="0,35,0,0"  NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
